{
  "servers": {
    "playwright": {
      "command": "npx",
      "args": [
        "@playwright/mcp@latest"
      ]
    },
    "Figma": {
      "url": "http://127.0.0.1:3845/mcp",
      "type": "http"
    },
    "microsoft/playwright-mcp": {
      "type": "stdio",
      "command": "npx",
      "args": [
        "-y",
        "@playwright/mcp@latest"
      ],
      "x-metadata": {
        "registry": {
          "api": {
            "baseUrl": "https://api.mcp.github.com",
            "version": "v0.1"
          },
          "mcpServer": {
            "name": "microsoft/playwright-mcp",
            "version": "0.0.1-seed"
          }
        }
      }
    },
    "com.figma.mcp/mcp": {
      "type": "http",
      "url": "https://mcp.figma.com/mcp",
      "x-metadata": {
        "registry": {
          "api": {
            "baseUrl": "https://api.mcp.github.com",
            "version": "v0.1"
          },
          "mcpServer": {
            "name": "com.figma.mcp/mcp",
            "version": "1.0.3"
          }
        }
      }
    },
    "com.stackoverflow.mcp/mcp": {
      "type": "http",
      "url": "https://mcp.stackoverflow.com",
      "x-metadata": {
        "registry": {
          "api": {
            "baseUrl": "https://api.mcp.github.com",
            "version": "v0.1"
          },
          "mcpServer": {
            "name": "com.stackoverflow.mcp/mcp",
            "version": "1.1.3"
          }
        }
      }
    }
  }
}